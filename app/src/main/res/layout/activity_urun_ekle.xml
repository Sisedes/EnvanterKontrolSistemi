<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E5E9F1"
    tools:context=".UrunBilgi.UrunEkle">
    <TextView
        android:id="@+id/txt_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="ÜRÜN EKLE"
        android:textSize="25dp"
        android:textColor="#75659D"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_urunid"
        android:layout_width="360dp"
        android:layout_height="49dp"
        android:layout_marginTop="112dp"
        android:background="@drawable/bg_tv2"
        android:drawableLeft="@drawable/icon_bos"
        android:ems="10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_urunisim"
        android:layout_width="360dp"
        android:layout_height="49dp"
        android:layout_marginTop="200dp"
        android:background="@drawable/bg_tv2"
        android:drawableLeft="@drawable/icon_bos"
        android:ems="10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_urunfiyat"
        android:layout_width="161dp"
        android:layout_height="49dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="288dp"
        android:background="@drawable/bg_tv2"
        android:drawableLeft="@drawable/icon_bos"
        android:ems="10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_urunkaydet"
        android:layout_width="175dp"
        android:layout_height="53dp"
        android:layout_marginTop="372dp"
        android:background="@drawable/bg_btn_urunler"
        android:textColor="@color/white"
        android:text="Kaydet"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.926"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:text="Ürün Adı"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/et_urunisim"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:text="Ürün ID"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/et_urunid"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="Fiyat"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/et_urunfiyat"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/et_urunstok"
        android:layout_width="180dp"
        android:layout_height="52dp"
        android:layout_marginTop="124dp"
        android:background="@drawable/bg_tv2"
        android:drawableLeft="@drawable/icon_bos"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/et_urunfiyat"
        app:layout_constraintTop_toBottomOf="@+id/et_urunid" />

    <EditText
        android:id="@+id/et_urundepoid"
        android:layout_width="164dp"
        android:layout_height="55dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/bg_tv2"
        android:drawableLeft="@drawable/icon_bos"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.087"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_urunfiyat" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="152dp"
        android:text="Stok"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/et_urunstok"
        app:layout_constraintStart_toEndOf="@+id/textView21" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="Depo ID"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/et_urundepoid"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>